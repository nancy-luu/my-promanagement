{
  "name": "the-dojo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/x-date-pickers": "^6.16.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "firebase": "8.5",
    "lottie-react": "^2.4.0",
    "react": "^17.0.2",
    "react-big-calendar": "^1.8.4",
    "react-dom": "^17.0.2",
    "react-flatpickr": "^3.10.13",
    "react-icons": "^4.11.0",
    "react-router-dom": "5.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node --loader esm node_modules/jest/bin/jest.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-flow": "^7.23.3",
    "@babel/preset-modules": "^0.1.6",
    "@babel/preset-react": "^7.23.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-styled-components": "^2.1.4",
    "esm": "^3.2.25",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "tailwindcss": "^3.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "type": "module",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/node_modules/jest-transform-stub"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|webp|svg|css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "extensionsToTreatAsEsm": [
      ".jsx"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array)",
      "/node_modules/(?!@mui/x-date-pickers/)"
    ]
  }
}
